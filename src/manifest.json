{
  "name": "Watch on Odysee",
  "version": "1.6.0",
  "permissions": [
    "https://www.youtube.com/watch?v=*",
    "https://www.youtube.com/channel/*",
    "https://invidio.us/channel/*",
    "https://invidio.us/watch?v=*",
    "https://api.lbry.com/*",
    "https://lbry.tv/*",
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": ["scripts/storageSetup.js", "scripts/tabOnUpdated.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Watch on Odysee",
    "default_popup": "popup/popup.html"
  },
  "web_accessible_resources": ["popup.html", "tools/YTtoLBRY.html"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "manifest_version": 2
}
